package com.coforge.training.airline.model;

import javax.persistence.Entity;
import javax.persistence.Table;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Entity
@Table(name = "upi")
@Data
@AllArgsConstructor
@NoArgsConstructor
public class UPI {

	private String upi;
	
	private int code;
	
}
